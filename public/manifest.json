{
  "short_name": "Europa",
  "name": "Jupiter Europa",
  "description": "Connect to Jupiter Blockchain Network",
  "version": "1.2.0",
  "manifest_version": 2,
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "browser_action": {
    "default_icon": "logo512.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "static/background.js"
    ]
  },
  "permissions": [
    "storage",
    "clipboardWrite",
    "activeTab",
    "notifications",
    "*://*/*"
  ],
  "externally_connectable": {
    "ids": [
      "gafpmgjlgaocgmlggjbpjmpedlebgnej"
    ],
    "matches": [
      "*://localhost/*",
      "*://*.jup.io/*"
    ],
    "accepts_tls_channel_id,": false
  }
}